package colec;

public class Paciente {
	private int codPaciente, dniPaciente, tlfPaciente, corrPaciente, sexPaciente;
	private String nomPaciente;
	public Paciente(int codPaciente, int dniPaciente, int tlfPaciente, int corrPaciente, int sexPaciente,
			String nomPaciente) {
		super();
		this.codPaciente = codPaciente;
		this.dniPaciente = dniPaciente;
		this.tlfPaciente = tlfPaciente;
		this.corrPaciente = corrPaciente;
		this.sexPaciente = sexPaciente;
		this.nomPaciente = nomPaciente;
	}
	public int getCodPaciente() {
		return codPaciente;
	}
	public void setCodPaciente(int codPaciente) {
		this.codPaciente = codPaciente;
	}
	public int getDniPaciente() {
		return dniPaciente;
	}
	public void setDniPaciente(int dniPaciente) {
		this.dniPaciente = dniPaciente;
	}
	public int getTlfPaciente() {
		return tlfPaciente;
	}
	public void setTlfPaciente(int tlfPaciente) {
		this.tlfPaciente = tlfPaciente;
	}
	public int getCorrPaciente() {
		return corrPaciente;
	}
	public void setCorrPaciente(int corrPaciente) {
		this.corrPaciente = corrPaciente;
	}
	public int getSexPaciente() {
		return sexPaciente;
	}
	public void setSexPaciente(int sexPaciente) {
		this.sexPaciente = sexPaciente;
	}
	public String getNomPaciente() {
		return nomPaciente;
	}
	public void setNomPaciente(String nomPaciente) {
		this.nomPaciente = nomPaciente;
	}
	
}
